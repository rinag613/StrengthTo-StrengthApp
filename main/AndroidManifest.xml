<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.strengthtostrength">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StrengthToStrength">
        <activity
            android:name=".activities.victimsResources.BeginningToHeal"
            android:exported="false" />
        <activity
            android:name=".activities.victimsResources.CopingAndConnection"
            android:exported="false" />
        <activity
            android:name=".activities.victimsResources.ISurvived"
            android:exported="false" />
        <activity
            android:name=".activities.victimsResources.VictimsResourcesHome"
            android:exported="false" />
        <activity
            android:name=".activities.OrganizationResources"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.StrengthToStrength" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.StrengthToStrength">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>